# apache2
virtualhosts:
  - name: pagina1
    datos:
      nameserver: www.pagina1.org
      documentroot: /srv/www/pagina1
      errorlog: error_pagina1
      accesslog: access_pagina1
  - name: pagina2
    datos:
      nameserver: www.pagina2.org
      documentroot: /srv/www/pagina2
      errorlog: error_pagina2
      accesslog: access_pagina2


